{
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },

    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "modules": true,
        "jsx": true
      }
    },

    "plugins": ["react", "import"],

    "extends": "airbnb",

    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.js"
        }
      }
    }

}
